doctype html
html(lang="en")
  head
    title Oregon State Government Website
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    link(rel="stylesheet", href="style.css")
    script(type="module").
      document.documentElement.classList.remove('no-js');
      document.documentElement.classList.add('js');
    link(rel="stylesheet", href="styles.css")
  body
    .wrapper
      .header
      .nav.navbar
        ul.navbar-nav
          li.nav-item
            a.nav-link(href="index.html") Home
          li.nav-item
            a.nav-link(href="salem.html") Salem
          li.nav-item
            a.nav-link(href="portland.html") Portland
          li.nav-item
            a.nav-link(href="eugene.html") Eugene
      .main
      .aside
      .footer
    script(src="index.js" async)
